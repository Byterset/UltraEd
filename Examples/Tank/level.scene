   "script": "typedef struct $_Bullet\n{\n\tVector3 dir;\n\tVector3 initPos;\n} $Bullet;\n\nvoid $Notify(Actor *self, const char *message, void *data)\n{\n\tif(message == \"Fire\" && data != NULL)\n\t{\n\t\tVector3 *dir = (Vector3*)data;\n\t\tif(dir != NULL && self->dynamic != NULL)\n\t\t{\n\t\t\t(($Bullet *)self->dynamic)->dir = *dir;\n\t\t\t(($Bullet *)self->dynamic)->initPos = self->position;\n\t\t}\n\t}\n}\n\nvoid $Start(Actor *self)\n{\n\tself->dynamic = ($Bullet *)malloc(sizeof($Bullet));\n\tself->notify = $Notify;\n}\n\nvoid $Update(Actor *self)\n{\n\t$Bullet *bullet = ($Bullet *)self->dynamic;\n\tif(bullet != NULL)\n\t{\n\t\tself->position = vec3_add(self->position, bullet->dir);\n\t\t\n\t\tif(vec3_dist(self->position, bullet->initPos) > 20)\n\t\t\tDestroy(self);\n\t}\n}\n",
